{
    "name": "JanKyselica/SeedOrganizer",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "JanKyselica",
        "SeedOrganizer"
    ],
    "homepage": "https://github.com/JanKyselica/SeedOrganizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Kyselica",
            "email": "kyselica.jan@gmail.com",
            "homepage": "https://github.com/JanKyselica/SeedOrganizer",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JanKyselica\\SeedOrganizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JanKyselica\\SeedOrganizer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
